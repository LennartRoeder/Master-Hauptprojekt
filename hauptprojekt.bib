	mendeley-groups = {GIS Big Data},
	number = {3},
	pages = {262--267},
	title = {{Big data and human geography: Opportunities, challenges and risks}},
	url = {http://journals.sagepub.com/doi/pdf/10.1177/2043820613513388 http://dhg.sagepub.com/content/3/3/262.short{\%}5Cnhttp://dhg.sagepub.com/lookup/doi/10.1177/2043820613513388},
	volume = {3},

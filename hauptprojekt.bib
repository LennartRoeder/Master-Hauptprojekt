	journal = {Big Data Research},
	keywords = {Complex event processing,Geospatial big data,Spatial big data,Spatial online analytical processing},
	mendeley-groups = {GIS Big Data},
	number = {2},
	pages = {74--81},
	title = {{Geospatial Big Data: Challenges and Opportunities}},
	url = {www.elsevier.com/locate/bdr http://www.sciencedirect.com/science/article/pii/S2214579615000040 http://linkinghub.elsevier.com/retrieve/pii/S2214579615000040},
